  "dependencies": {
    "loader-utils": "1.1.0",
    "prismjs": "1.6.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "1.3.1",
    "conventional-github-releaser": "1.1.3",
    "eslint-config-valorsoft": "0.1.0",
    "gh-pages": "0.12.0"
  }
